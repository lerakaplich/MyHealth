<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kaplich.myhealth">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.MyHealth">

    <activity

        android:name=".MainActivity"
        android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

    <activity android:name=".pages.LoginActivity" />
    <activity android:name=".pages.ProfileActivity"/>
    <activity android:name=".pages.RegistrationActivity"/>
    <activity android:name=".pages.MainPageActivity"
        tools:ignore="Instantiatable" />
    <activity android:name=".addendum.AddAnalysisActivity"/>
    <activity android:name=".addendum.AddAppointmentActivity"/>
    <activity android:name=".addendum.AddEventActivity"/>
    <activity android:name=".addendum.AddMedicationActivity"/>
    <activity android:name=".addendum.ProfileEditActivity"/>
</application>
</manifest>